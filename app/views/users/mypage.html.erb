<div class="page-title">
  <h3>評価結果</h3>
</div>
<div class="table-title center-align">
  <%=current_user.superior_user.name %> さんからの評価
</div>
<table class="bordered highlight answer_table">
  <tbody >
    <% @answers.each do |answer| %>
      <tr>
        <td class="question-text" colspan="2"><%= answer.question_id %> ： <%= answer.question.text %></td>
      </tr>
      <tr>
        <td>評価ポイント：<%= answer.rate %></td>
        <td class="question_alternative-text">
          <%= @question_alternative.text %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
