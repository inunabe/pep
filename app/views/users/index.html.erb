<div class="page-title">  <h3>ユーザーリスト</h3></div><% if current_user.admin? %>  <div class="actions right-align">    <%= link_to "ユーザー登録", new_user_path, :class => "btn question-link waves-effect waves-light"%>  </div><% else %><% end %>  <table class="bordered highlight">    <tbody>      <% @users.each_with_index do |user,i| %>        <tr>          <td class="user-name"><%= "#{i + 1}" %></td>        <% if current_user.admin? %>          <td class="user-name"><%= link_to "#{user.name}", edit_user_path(user) %>          </td>            <% if user.grade.present? && current_user.admin? %>          <td class="user-name">            <%= user.grade.department.name %>            &thinsp;            <%= user.grade.rank %>&thinsp;&thinsp;等級          </td>            <% else %>          <td></td>            <% end %>          <td class="user-email"><%= user.email %></td>          <td class="user-link"><%= link_to '評価', "/answers/#{user.id}/select_period" %></td>          <td class="user-link"><%= link_to 'ユーザー消去', user_path(user), method: :delete, data: { confirm: '本当に消去しますか?' } %></td>        <% else %>          <td class="user-name"><%= user.name %></td>          <td class="user-email"><%= user.email %></td>        <% end %>        </tr>      <% end %>    </tbody>  </table>  <!-- google-mapAPIここから --><!--   <div id="fukuei-map">  </div>  <div class="map-title">    福栄肥料株式会社  </div>  <div class="fukuei-address">    <span class="postal-number">〒660-0882</span><br>    兵庫県尼崎市昭和南通三丁目26番地 松本ビル6F  </div>  <script async defer    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGjDT8Us6vibhEvOAz0HQg2K5WfDS1JRg&callback=initMap">    </script> --><!-- ここまで -->