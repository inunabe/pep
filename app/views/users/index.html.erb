<div class="container">
<p id="notice"><%= notice %></p>

<h3>ユーザーリスト</h3>

<table>
  <thead>
    <tr>
      <th>メールアドレス</th>
      <!-- <th>password</th> -->
     <!--  <th>Salt</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <!-- <td><%= user.crypted_password %></td> -->
        <!-- <td><%= user.salt %></td> -->
        <td><%= link_to '詳細', user_path(user) %></td>
        <td><%= link_to '編集', edit_user_path(user) %></td>
        <td><%= link_to 'ユーザー消去', user_path(user), method: :delete, data: { confirm: '本当に消去しますか?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新規登録', new_user_path %>
<%= link_to '質問項目登録', new_question_path, class: 'get' %>
<%= link_to '質問項目一覧', questions_path %>

</div>
