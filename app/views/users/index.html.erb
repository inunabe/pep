<div class="page-title">  <h3>ユーザーリスト</h3></div>  <table class="bordered highlight">    <tbody>      <% @users.each do |user| %>        <tr>          <% if current_user && current_user.admin? %>          <td><%= user.name %></td>          <td><%= user.email %></td>          <td><%= link_to '編集', edit_user_path(user) %></td>          <td><%= link_to 'ユーザー消去', user_path(user), method: :delete, data: { confirm: '本当に消去しますか?' } %></td>      <% else %>          <td><%= user.name %></td>          <td><%= user.email %></td>      <% end %>        </tr>      <% end %>    </tbody>  </table>