<div class="page-title">  <h3>ユーザーリスト</h3></div>  <table class="bordered highlight">    <tbody>      <% @users.each do |user| %>        <tr>          <% if current_user.admin? %>          <td class="user-name"><%= user.name %></td>          <td class="user-email"><%= user.email %></td>          <td class="user-link"><%= link_to '編集', edit_user_path(user) %></td>          <td class="user-link"><%= link_to 'ユーザー消去', user_path(user), method: :delete, data: { confirm: '本当に消去しますか?' } %></td>      <% else %>          <td class="user-name"><%= user.name %></td>          <td class="user-email"><%= user.email %></td>      <% end %>        </tr>      <% end %>    </tbody>  </table>