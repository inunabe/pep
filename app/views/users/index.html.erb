<p id="notice"><%= notice %></p>

<h1>ユーザーリスト</h1>

<table>
  <thead>
    <tr>
      <th>メールアドレス</th>
      <!-- <th>password</th> -->
     <!--  <th>Salt</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <!-- <td><%= user.crypted_password %></td> -->
        <!-- <td><%= user.salt %></td> -->
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: '本当に消去しますか?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

