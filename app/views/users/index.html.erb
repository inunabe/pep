  <h3>ユーザーリスト</h3>  <table class="striped">    <thead>      <tr>        <th>氏名</th>        <th>メールアドレス</th>      </tr>    </thead>    <tbody>      <% @users.each do |user| %>        <tr>          <% if current_user && current_user.admin? %>          <td><%= user.name %></td>          <td><%= user.email %></td>          <td><%= link_to '編集', edit_user_path(user) %></td>          <td><%= link_to 'ユーザー消去', user_path(user), method: :delete, data: { confirm: '本当に消去しますか?' } %></td>      <% else %>          <td><%= user.name %></td>          <td><%= user.email %></td>      <% end %>        </tr>      <% end %>    </tbody>  </table>  <br>      <div class=" btn btn-menu  ">        <%= link_to 'ユーザー登録', new_user_path, class: "link" %>      </div>      <div class=" btn btn-menu ">        <%= link_to '質問項目登録', new_question_path, method: :get %>      </div>      <div class=" btn btn-menu ">        <%= link_to '質問項目一覧', questions_path %>      </div>